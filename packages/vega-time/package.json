{
  "name": "vega-time",
  "version": "2.0.2",
  "description": "JavaScript date/time utilities for Vega.",
  "keywords": [
    "vega",
    "date",
    "time",
    "utilities"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-time.js",
  "module": "build/vega-time.module.js",
  "unpkg": "build/vega-time.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals d3-array=d3,d3-time=d3,vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-array": "^2.5.1",
    "d3-time": "^1.1.0",
    "vega-util": "^1.15.0"
  }
}
