{
  "name": "vega-selections",
  "version": "5.1.2",
  "description": "Vega expression functions for Vega-Lite selections.",
  "keywords": [
    "vega",
    "expression",
    "selections"
  ],
  "author": "Arvind Satyanarayan (http://arvindsatya.com)",
  "license": "BSD-3-Clause",
  "source": "index.js",
  "main": "build/vega-selections.js",
  "module": "build/vega-selections.module.js",
  "unpkg": "build/vega-selections.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals vega-expression=vega,vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "vega-expression": "^2.6.6",
    "vega-util": "^1.15.0"
  }
}
