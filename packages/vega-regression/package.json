{
  "name": "vega-regression",
  "version": "1.0.7",
  "description": "Regression transform for Vega dataflows.",
  "keywords": [
    "vega",
    "regression",
    "loess"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-regression.js",
  "module": "build/vega-regression.module.js",
  "unpkg": "build/vega-regression.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals d3-array=d3,vega-dataflow=vega,vega-statistics=vega,vega-util=vega --name vega.transforms",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-array": "^2.5.1",
    "vega-dataflow": "^5.7.1",
    "vega-statistics": "^1.7.7",
    "vega-util": "^1.15.0"
  },
  "devDependencies": {
    "vega-transforms": "*"
  }
}
