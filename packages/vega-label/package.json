{
  "name": "vega-label",
  "version": "0.0.4",
  "description": "Label layout transform for Vega dataflows.",
  "keywords": [
    "vega",
    "label",
    "layout"
  ],
  "license": "BSD-3-Clause",
  "author": "UW Interactive Data Lab (http://idl.cs.washington.edu)",
  "maintainers": [
    "Chanwut Kittivorawong"
  ],
  "source": "index.js",
  "main": "build/vega-label.js",
  "module": "build/vega-label.module.js",
  "unpkg": "build/vega-label.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals vega-canvas=vega,vega-dataflow=vega,vega-scenegraph=vega,vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "vega-canvas": "^1.2.3",
    "vega-dataflow": "^5.7.1",
    "vega-scenegraph": "^4.9.0",
    "vega-util": "^1.15.0"
  },
  "devDependencies": {
    "vega-transforms": "*"
  }
}
