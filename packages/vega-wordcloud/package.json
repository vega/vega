{
  "name": "vega-wordcloud",
  "version": "4.1.1",
  "description": "Wordcloud layout transform for Vega dataflows.",
  "keywords": [
    "vega",
    "wordcloud",
    "layout"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-wordcloud.js",
  "module": "build/vega-wordcloud.module.js",
  "unpkg": "build/vega-wordcloud.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals vega-canvas=vega,vega-dataflow=vega,vega-scale=vega,vega-statistics=vega,vega-util=vega --name vega.transforms",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "vega-canvas": "^1.2.3",
    "vega-dataflow": "^5.7.1",
    "vega-scale": "^7.0.1",
    "vega-statistics": "^1.7.7",
    "vega-util": "^1.15.0"
  },
  "devDependencies": {
    "vega-transforms": "*"
  }
}
