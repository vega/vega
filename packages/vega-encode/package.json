{
  "name": "vega-encode",
  "version": "4.8.1",
  "description": "Visual encoding transforms for Vega dataflows.",
  "keywords": [
    "vega",
    "encode",
    "axes",
    "legends"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-encode.js",
  "module": "build/vega-encode.module.js",
  "unpkg": "build/vega-encode.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals d3-array=d3,d3-interpolate=d3,vega-dataflow=vega,vega-scale=vega,vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-array": "^2.5.1",
    "d3-interpolate": "^1.4.0",
    "vega-dataflow": "^5.7.1",
    "vega-scale": "^7.0.1",
    "vega-util": "^1.15.0"
  },
  "devDependencies": {
    "vega-transforms": "*"
  }
}
