{
  "name": "vega",
  "version": "5.14.0",
  "description": "The Vega visualization grammar.",
  "keywords": [
    "vega",
    "visualization",
    "interaction",
    "dataflow",
    "library",
    "data",
    "d3"
  ],
  "license": "BSD-3-Clause",
  "author": "UW Interactive Data Lab (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega.node.js",
  "module": "build/vega.module.js",
  "unpkg": "build/vega.umd.js",
  "jsdelivr": "build/vega.umd.js",
  "types": "index.d.ts",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "yarn build:es && yarn build:umd && yarn build:core && yarn build:node && node schema-copy",
    "microbundle:base": "microbundle --name vega --global vega-crossfilter=vega,vega-dataflow=vega,vega-encode=vega,vega-event=vega,vega-expression=vega,vega-force=vega,vega-format=vega,vega-functions=vega,vega-geo=vega,vega-hierarchy=vega,vega-loader=vega,vega-parser=vega,vega-projection=vega,vega-regression=vega,vega-runtime=vega,vega-scale=vega,vega-scenegraph=vega,vega-statistics=vega,vega-time=vega,vega-transforms=vega,vega-typings=vega,vega-util=vega,vega-view=vega,vega-view=vega,vega-voronoi=vega,vega-wordcloud=vega",
    "build:es": "yarn microbundle:base -f modern,es -o build/vega.js",
    "build:umd": "yarn microbundle:base -f umd --external none -o build/vega.js",
    "build:core": "yarn microbundle:base -f umd --external d3-color,d3-dispatch,d3-dsv,d3-force,d3-format,d3-geo,d3-hierarchy,d3-interpolate,d3-path,d3-shape,d3-time,d3-time-format,d3-timer,topojson-client -o build/vega-core.js",
    "build:node": "yarn microbundle:base -f cjs --target node -o build/vega.node.js",
    "pretest": "yarn build",
    "test": "TZ=America/Los_Angeles tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags && ./schema-deploy.sh"
  },
  "dependencies": {
    "vega-crossfilter": "~4.0.3",
    "vega-dataflow": "~5.7.1",
    "vega-encode": "~4.8.1",
    "vega-event-selector": "~2.0.4",
    "vega-expression": "~2.6.6",
    "vega-force": "~4.0.5",
    "vega-format": "~1.0.2",
    "vega-functions": "~5.7.2",
    "vega-geo": "~4.3.5",
    "vega-hierarchy": "~4.0.7",
    "vega-loader": "~4.3.1",
    "vega-parser": "~6.0.2",
    "vega-projection": "~1.4.3",
    "vega-regression": "~1.0.7",
    "vega-runtime": "~6.1.1",
    "vega-scale": "~7.0.1",
    "vega-scenegraph": "~4.9.0",
    "vega-statistics": "~1.7.7",
    "vega-time": "~2.0.2",
    "vega-transforms": "~4.9.1",
    "vega-typings": "~0.18.1",
    "vega-util": "~1.15.0",
    "vega-view": "~5.8.1",
    "vega-view-transforms": "~4.5.6",
    "vega-voronoi": "~4.1.3",
    "vega-wordcloud": "~4.1.1"
  },
  "devDependencies": {
    "vega-schema": "*"
  }
}
