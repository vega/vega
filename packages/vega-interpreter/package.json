{
  "name": "vega-interpreter",
  "version": "1.0.1",
  "description": "CSP-compliant interpreter for Vega expressions.",
  "keywords": [
    "vega",
    "expression",
    "interpreter",
    "runtime"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-interpreter.js",
  "module": "build/vega-interpreter.module.js",
  "unpkg": "build/vega-interpreter.umd.js",
  "types": "index.d.ts",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --name vega --extend",
    "pretest": "yarn build",
    "test": "TZ=America/Los_Angeles tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "devDependencies": {
    "vega": "*"
  }
}
