{
  "name": "vega-loader",
  "version": "4.3.1",
  "description": "Network request and file loading utilities.",
  "keywords": [
    "vega",
    "loader",
    "file",
    "http",
    "fetch",
    "json",
    "csv",
    "tsv",
    "topojson",
    "parse",
    "type",
    "inference"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-loader.node.js",
  "module": "build/vega-loader.module.js",
  "unpkg": "build/vega-loader.umd.js",
  "jsdelivr": "build/vega-loader.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "yarn build:browser && yarn build:node",
    "microbundle:base": "microbundle --globals d3-dsv=d3,vega-format=vega,vega-util=vega,topojson-client:topojson --name vega",
    "build:browser": "yarn microbundle:base -f modern,es,umd -i index.browser.js -o build/vega-loader.js",
    "build:node": "yarn microbundle:base --target node -f cjs -o build/vega-loader.node.js",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-dsv": "^1.2.0",
    "node-fetch": "^2.6.0",
    "topojson-client": "^3.1.0",
    "vega-format": "^1.0.2",
    "vega-util": "^1.15.0"
  },
  "browser": {
    "./build/vega-loader.node.js": "./build/vega-loader.js",
    "./index.js": "./index.browser.js"
  }
}
