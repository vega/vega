{
  "name": "vega-projection",
  "version": "1.4.3",
  "description": "Projections for cartographic mapping.",
  "keywords": [
    "vega",
    "geo",
    "projection"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-projection.js",
  "module": "build/vega-projection.module.js",
  "unpkg": "build/vega-projection.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals d3-geo=d3,d3-geo-projection=d3 --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-geo": "^1.12.1",
    "d3-geo-projection": "^2.9.0"
  }
}
