{
  "name": "vega-dataflow",
  "version": "5.7.1",
  "description": "Reactive dataflow processing.",
  "keywords": [
    "vega",
    "dataflow",
    "reactive",
    "streaming",
    "data"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-dataflow.js",
  "module": "build/vega-dataflow.module.js",
  "unpkg": "build/vega-dataflow.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals vega-format=vega,vega-loader=vega,vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "vega-format": "^1.0.2",
    "vega-loader": "^4.3.1",
    "vega-util": "^1.15.0"
  }
}
