  },
  "devDependencies": {
    "vega-crossfilter": "4.1.4",
    "vega-encode": "4.10.3",
    "vega-force": "4.2.3",
    "vega-geo": "4.4.4",
    "vega-hierarchy": "4.1.4",
    "vega-label": "1.3.2",
    "vega-regression": "1.3.2",
    "vega-transforms": "4.12.2",
    "vega-view-transforms": "4.6.2",
    "vega-voronoi": "4.2.5",
    "vega-wordcloud": "4.1.7"
  }
