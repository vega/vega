{
  "name": "vega-scenegraph",
  "version": "4.9.0",
  "description": "Vega scenegraph and renderers.",
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-scenegraph.js",
  "module": "build/vega-scenegraph.module.js",
  "unpkg": "build/vega-scenegraph.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "schema": "node schema > build/vega-scenegraph-schema.json",
    "build": "microbundle --globals d3-path=d3,d3-shape=d3,vega-canvas=vega,vega-loader=vega,vega-scale=vega,vega-util=vega --name vega",
    "postbuild": "yarn schema",
    "pretest": "yarn build",
    "test": "tape -r ./test/__init__ 'test/**/*-test.js' && yarn xmllint",
    "xmllint": "xmllint --noout ./test/resources/svg/*.svg",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-path": "^1.0.9",
    "d3-shape": "^1.3.7",
    "vega-canvas": "^1.2.3",
    "vega-loader": "^4.3.1",
    "vega-scale": "^7.0.1",
    "vega-util": "^1.15.0"
  }
}
