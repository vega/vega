{
  "name": "vega-hierarchy",
  "version": "4.0.7",
  "description": "Hierarchical layout transforms for Vega dataflows.",
  "keywords": [
    "vega",
    "hierarchy",
    "layout",
    "tree",
    "treemap",
    "cluster",
    "pack"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-hierarchy.js",
  "module": "build/vega-hierarchy.module.js",
  "unpkg": "build/vega-hierarchy.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals d3-hierarchy=d3,vega-dataflow=vega,vega-util=vega --name vega.transforms",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-hierarchy": "^1.1.9",
    "vega-dataflow": "^5.7.1",
    "vega-util": "^1.15.0"
  },
  "devDependencies": {
    "vega-transforms": "*"
  }
}
