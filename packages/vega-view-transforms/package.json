{
  "name": "vega-view-transforms",
  "version": "4.5.6",
  "description": "View-specific transforms for Vega dataflows.",
  "keywords": [
    "vega",
    "view",
    "layout"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-view-transforms.js",
  "module": "build/vega-view-transforms.module.js",
  "unpkg": "build/vega-view-transforms.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals vega-dataflow=vega,vega-scenegraph=vega,vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "vega-dataflow": "^5.7.1",
    "vega-scenegraph": "^4.9.0",
    "vega-util": "^1.15.0"
  },
  "devDependencies": {
    "vega-transforms": "*"
  }
}
