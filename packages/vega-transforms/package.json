{
  "name": "vega-transforms",
  "version": "4.9.1",
  "description": "Data processing transforms for Vega dataflows.",
  "keywords": [
    "vega",
    "dataflow",
    "transforms",
    "filter",
    "aggregate",
    "project",
    "window",
    "sort",
    "bin",
    "data"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-transforms.js",
  "module": "build/vega-transforms.module.js",
  "unpkg": "build/vega-transforms.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals d3-array=d3,vega-dataflow=vega,vega-statistics=vega,vega-time=vega,vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-array": "^2.5.1",
    "vega-dataflow": "^5.7.1",
    "vega-statistics": "^1.7.7",
    "vega-time": "^2.0.2",
    "vega-util": "^1.15.0"
  }
}
