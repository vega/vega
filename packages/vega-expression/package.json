{
  "name": "vega-expression",
  "version": "2.6.6",
  "description": "Vega expression parser and code generator.",
  "keywords": [
    "vega",
    "expression",
    "parser",
    "codegen"
  ],
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "license": "BSD-3-Clause",
  "source": "index.js",
  "main": "build/vega-expression.js",
  "module": "build/vega-expression.module.js",
  "unpkg": "build/vega-expression.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "vega-util": "^1.15.0"
  }
}
