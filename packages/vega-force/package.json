{
  "name": "@datatitian/vega-force",
  "version": "4.2.0",
  "description": "Force simulation transform for Vega dataflows.",
  "keywords": [
    "vega",
    "force"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "main": "build/vega-force.js",
  "module": "build/vega-force.module.js",
  "unpkg": "build/vega-force.min.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "rollup -c --config-transform",
    "build:worker": "rollup -p @rollup/plugin-node-resolve -f umd -n vega.transforms -o build/vega-force-worker.js -- index-worker.js",
    "pretest": "yarn build --config-test",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build"
  },
  "dependencies": {
    "d3-force": "^2.1.1",
    "vega-dataflow": "^5.7.3",
    "vega-util": "^1.15.2",
    "@rollup/plugin-node-resolve": "^8.1.0"
  },
  "devDependencies": {
    "vega-transforms": "*"
  }
}
