{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "checkJs": true,
    "rootDir": "src",
    "noEmit": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "build/types"
  },
  "include": [
    "src/is*.js",
    "src/toBoolean.js",
    "src/toNumber.js",
    "src/toString.js",
    "src/toDate.js",
    "src/toSet.js",
    "src/truncate.js",
    "src/pad.js",
    "src/repeat.js",
    "src/splitAccessPath.js",
    "src/stringValue.js"
  ]
}
