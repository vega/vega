{
  "name": "vega-projection-extended",
  "version": "2.0.4",
  "description": "Extended projections for cartographic mapping.",
  "keywords": [
    "vega",
    "geo",
    "projection",
    "extended"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-projections.js",
  "module": "build/vega-projections.module.js",
  "unpkg": "build/vega-projections.umd.js",
  "jsdelivr": "build/vega-projections.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "yarn build:main && yarn build:umd && yarn build:core",
    "microbundle:base": "microbundle --globals vega-projection=vega --name vega",
    "build:main": "yarn microbundle:base -f modern,es,cjs",
    "build:umd": "yarn microbundle:base -f umd --external none",
    "build:core": "microbundle --globals d3-array=d3,d3-geo=d3,vega-projection=vega --name vega --external d3-array,d3-geo -f umd -o build/vega-projections-core.umd.js",
    "prepublishOnly": "yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "d3-geo-projection": "^2.9.0",
    "vega-projection": "^1.4.2"
  }
}
