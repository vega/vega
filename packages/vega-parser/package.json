{
  "name": "vega-parser",
  "version": "6.0.3",
  "description": "Parse Vega specifications to runtime dataflows.",
  "keywords": [
    "vega",
    "parser",
    "dataflow",
    "reactive"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "source": "index.js",
  "main": "build/vega-parser.js",
  "module": "build/vega-parser.module.js",
  "unpkg": "build/vega-parser.umd.js",
  "repository": "vega/vega",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "microbundle --globals vega-dataflow=vega,vega-event-selector=vega,vega-functions=vega,vega-scale=vega,vega-util=vega --name vega",
    "pretest": "yarn build",
    "test": "tape 'test/**/*-test.js'",
    "prepublishOnly": "yarn test && yarn build",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "vega-dataflow": "^5.7.1",
    "vega-event-selector": "^2.0.4",
    "vega-functions": "^5.7.1",
    "vega-scale": "^7.0.1",
    "vega-util": "^1.15.0"
  }
}
