{%
  assign types = include.t | split: "|"   %}{%
  for t in types   %}{%
    assign suffix = t | slice: -2, 2   %}{%
    assign array = suffix == "[]"   %}{%
    assign type = t | replace: "[]", ""   %}{%
    if forloop.index > 1   %}{%
      include or   %}{%
    endif   %}{%
    capture link   %}<a href="{{ site.baseurl }}/docs/types/#{{ type }}">{{ type }}</a>{%
    endcapture   %}{%
    if suffix == "[]"   %}{%
      include array t=link   %}{%
    else   %}{{ link }}{%
    endif   %}{%
  endfor   %}